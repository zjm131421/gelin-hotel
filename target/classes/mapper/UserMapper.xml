<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gelin.hotel.mapper.UserMapper">

    <select id="getByUsername" parameterType="String" resultType="com.gelin.hotel.entity.HotelUser">
        select id,username,password,name from hotel_users where username = #{username}
    </select>

</mapper>
